#include<stdio.h>
void main()
{
    int n,i,sum;
    int max,min;
    printf("Enter the starting number:");
    scanf("%d",&min);
    printf("Enter the ending number:");
    scanf("%d",&max);
    for(n=min;n<max;n++)
    {
        i=1;
        sum=0;
        while(i<n)
        {
        if(n%i==0)
            sum=sum+i;
        i++;
        }
    if(sum==n) 
    {
        printf("All three digit perfect number is %d ",n);

    }
    }
    printf("\n");
}
